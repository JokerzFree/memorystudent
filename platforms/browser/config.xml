<?xml version='1.0' encoding='utf-8'?>
<widget id="com.grsu.studentbooklet" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
	<feature name="InAppBrowser">
	  <param name="android-package" value="org.apache.cordova.InAppBrowser" />
	</feature>
	<feature name="InAppBrowser">
	    <param name="ios-package" value="CDVInAppBrowser" />
	</feature>
    <name>Памятка первокурснику</name>
    <description>Приложение с краткой информацией для первокурсников</description>
    <author email="urbanovichwork@gmail.com" href="http://vk.com/jokerzfree">Urbanovich Andrew</author>
    <content src="www/index.html" />
    <icon src="icon.png" />
    <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser" />
    <access origin="*" browserOnly="true"/>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="stay-in-webview" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
